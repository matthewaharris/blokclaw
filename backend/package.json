{
  "name": "blokclaw-backend",
  "version": "0.1.0",
  "description": "BlokClaw API Registry Backend",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "migrate": "node migrations/run.js",
    "seed": "node scripts/seed.js",
    "seed:all": "node scripts/seed-comprehensive.js",
    "stats:backfill": "node scripts/backfill-stats.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch"
  },
  "keywords": [
    "api",
    "registry",
    "ai",
    "agents"
  ],
  "author": "Matthew Harris",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.2.2"
  }
}
